{
  "steps": [
    {
      "command": "reboot bootloader"
    },
    {
      "id": "connect-bootloader",
      "command": ["connect bootloader", "flashing unlock unlocked" ],
      "needUserGesture": true
    },
    {
      "id": "activate-developer-options",
      "needUserGesture": true
    },
    {
      "id": "enable-usb-file-transfer",
      "needUserGesture": true
    },
    {
      "id": "device-detection",
      "command": "connect adb",
      "needUserGesture": true
    },
    {
      "mode": "bootloader",
      "id": "connect-bootloader",
      "command": [
        "connect bootloader"
      ],
      "needUserGesture": true
    },
    {
      "id": "flashing",
      "command": [
        "flash dtbo dtbo-e-2.1-t-20240605406922-dev-coral.img",
        "flash boot recovery-e-2.1-t-20240605406922-dev-coral.img",
        "reboot"
      ]
    },
    {
      "id": "device-detection",
      "command": [
        "connect adb",
        "reboot recovery"
      ],
      "needUserGesture": true
    },
    {
      "id": "erase-data-recovery",
      "needUserGesture": true
    },
    {
      "id": "sideload",
      "command": "sideload rom.zip"
    },
    {
      "id": "sideload-ended",
      "needUserGesture": true
    },
    {
      "id": "connect-bootloader",
      "command": [
        "connect bootloader",
        "erase avb_custom_key",
        "flash avb_custom_key pkmd_pixel.bin",
        "flashing lock unlocked"
      ],
      "needUserGesture": true
    },
    {
      "id": "murena-cloud-account",
      "needUserGesture": true
    },
    {
      "id": "installation-complete"
    }
  ],
  "folder": [
    {
      "name": "recovery.img",
      "path": "assets/sources/coral/recovery-IMG-e-2.1-t-20240605406922-dev-coral.zip",
      "unzip": true
    },
    {
      "name": "rom.zip",
      "path": "assets/sources/coral/e-2.1-t-20240605406922-dev-coral.zip"
    },
    {
      "name": "pkmd_pixel.bin",
      "path": "assets/sources/coral/pkmd_pixel.bin"
    }
  ]
}