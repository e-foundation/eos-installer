{
  "android": 13,
  "steps": [
    {
      "command": ["reboot bootloader", "delay 5"]
    },
    {
      "id": "connect-bootloader",
      "command": ["connect bootloader", "flashing unlock unlocked" ],
      "needUserGesture": true
    },
    {
      "id": "unlocking-pixel",
      "needUserGesture": true,
      "command": ["delay 15", "connect bootloader"]
    },
    {
      "id": "flashing",
      "command": [
        
        "flash dtbo dtbo-e-2.2-t-20240715417772-stable-redfin.img",
	      "flash boot boot-e-2.2-t-20240715417772-stable-redfin.img",
        "flash vendor_boot recovery-e-2.2-t-20240715417772-stable-redfin.img"
      ]
    },
    {
      "id": "bootloader-to-recovery-manually",
      "needUserGesture": true
    },
    {
      "id": "format-device",
      "needUserGesture": true
    },
    {
      "id": "go-to-apply-update",
      "needUserGesture": true
    },
    {
      "id": "sideload",
      "command": "sideload pixel-5-rom.zip"
    },
    {
      "id": "sideload-ended",
      "needUserGesture": true
    },
    {
      "id": "connect-bootloader",
      "command": [
        "connect bootloader",
        "erase avb_custom_key",
        "flash avb_custom_key pkmd_pixel.bin",
        "flashing lock unlocked"
      ],
      "needUserGesture": true
    },
    {
      "id": "locking-pixel",
      "needUserGesture": true
    },
    {
      "id": "murena-cloud-account",
      "needUserGesture": true
    },
    {
      "id": "installation-complete"
    }
  ],
  "folder": [
    {
      "name": "pixel-5-recovery.img",
      "path": "https://images.ecloud.global/stable/redfin/recovery-IMG-e-2.2-t-20240715417772-stable-redfin.zip",
      "unzip": true
    },
    {
      "name": "pixel-5-rom.zip",
      "path": "https://images.ecloud.global/stable/redfin/e-2.2-t-20240715417772-stable-redfin.zip"    
    },
    {
      "name": "pkmd_pixel.bin",
      "path": "https://images.ecloud.global/stable/pkmd_pixel.bin"
    }
  ]
}