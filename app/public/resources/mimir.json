{
  "android": 14,
  "steps": [
    {
      "command": ["reboot bootloader", "delay 15"]
    },
    {
      "id": "connect-bootloader",
      "command": ["connect bootloader"],
      "needUserGesture": true
    },
    {
      "id": "unlocking-mimir",
      "needUserGesture": true,
      "command": ["flashing unlock", "delay 5"]
    },
    {
      "id": "flashing-recovery",
      "command": [
        "flash dtbo_a dtbo.img",
        "flash boot_a boot.img",
        "flash vendor_boot_a recovery.img --set-active=a",
        "reboot recovery"
      ]
    },
    {
      "id": "format-device",
      "needUserGesture": true
    },
    {
      "id": "go-to-apply-update",
      "needUserGesture": true
    },
    {
      "id": "sideload",
      "command": "sideload volla-tablet-rom.zip"
    },
    {
      "id": "sideload-ended",
      "needUserGesture": true
    },
    {
      "id": "connect-bootloader",
      "command": ["connect bootloader"],
      "needUserGesture": true
    },
    {
      "id": "locking-mimir",
      "needUserGesture": true,
      "command": ["flashing lock", "delay 5"]
    },
    {
      "id": "murena-workspace-account",
      "needUserGesture": true
    },
    {
      "id": "installation-complete"
    },
    {
      "command": ["reboot"]
    }
  ],
  "folder": [
    {
      "name": "volla-tablet-recovery-images.zip",
      "path": "https://images.ecloud.global/official/mimir/recovery-IMG-e-latest-a14-official-mimir.zip",
      "unzip": true,
      "mapping": {
        "^dtbo.*img$": "dtbo.img",
        "^boot.*img$": "boot.img",
        "^recovery.*img$": "recovery.img"
      }
    },
    {
      "name": "volla-tablet-rom.zip",
      "path": "https://images.ecloud.global/official/mimir/e-latest-a14-official-mimir.zip"
    }
  ]
}
