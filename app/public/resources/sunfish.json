{
  "android": 14,
  "steps": [
    {
      "command": "reboot bootloader"
    },
    {
      "id": "connect-bootloader",
      "command": ["connect bootloader", "flashing unlock unlocked" ],
      "needUserGesture": true
    },
    {
      "id": "unlocking-pixel",
      "needUserGesture": true,
      "command": "delay 15"
    },
    {
      "id": "flashing",
      "command": [
        "flash dtbo dtbo.img",
        "flash boot recovery.img"
      ]
    },
    {
      "id": "bootloader-to-recovery-manually",
      "needUserGesture": true
    },
    {
      "id": "format-device",
      "needUserGesture": true
    },
    {
      "id": "go-to-apply-update",
      "needUserGesture": true
    },
    {
      "id": "sideload",
      "command": "sideload pixel-4a-rom.zip"
    },
    {
      "id": "sideload-ended",
      "needUserGesture": true
    },
    {
      "id": "connect-bootloader",
      "command": [
        "connect bootloader",
        "reboot"
      ],
      "needUserGesture": true
    },
    {
      "id": "murena-workspace-account",
      "needUserGesture": true
    },
    {
      "id": "installation-complete"
    }
  ],
  "folder": [
    {
      "name": "pixel-4a-recovery.img",
      "path": "https://images.ecloud.global/community/sunfish/recovery-IMG-e-latest-u-community-sunfish.zip",
      "unzip": true,
      "mapping": {
        "^dtbo.*img$": "dtbo.img",
        "^recovery.*img$": "recovery.img"
      }
    },
    {
      "name": "pixel-4a-rom.zip",
      "path": "https://images.ecloud.global/community/sunfish/e-latest-u-community-sunfish.zip"
    }
  ]
}
